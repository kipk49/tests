<!--
Hello! This is test number 6 in Kip's test bank. It builds off of test 5, 
and is supposed to add d3ploy capabilites to the simple closed fuel cycle 
found in number 5. As with test 5, power demand is constant. D3ploy's 
only purpose here is to deploy SFRs in response to the decommissioning 
of the original LWR fleet.

In this case, LWRs burn only UOX fuel and SFRs burn only MOX.

Recipes for the various materials were taken from the transition-scenarios
repository in the ARFC main repository at https://github.com/arfc/transition-scenarios

Thanks!

Note: Only the Mine, Enrichment Plant, and SFR facilities are under d3ploy
control. All others have been set to arbitrarily high limits, so as not to
interfere with the d3ploy simulation.

TECH DETAILS:
__________________________________________________________________________
Start Date: Jan 2000
Duration: 120 years (1440 months)
Power Demand: 60 GW, constant
LWR Info: 1 GW, 40 year lifespan
SFR Info: 1 GW, 40 year lifespan
-->
<simulation>
	<archetypes>
		<spec>
			<lib>agents</lib>
			<name>NullRegion</name>
		</spec>
		<spec>
			<lib>agents</lib>
			<name>NullInst</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Source</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Enrichment</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Reactor</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Storage</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Separations</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Mixer</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Sink</name>
		</spec>
		<spec>
			<lib>d3ploy.demand_driven_deployment_inst</lib>
			<name>DemandDrivenDeploymentInst</name>
		</spec>
	</archetypes>

	<!-- Controls -->

	<control>
		<duration>1440</duration>
		<startmonth>1</startmonth>
		<startyear>2000</startyear>
	</control>

	<!-- Facilities -->

	<facility>
		<config>
			<Source>
				<outcommod>natural_u</outcommod>
				<outrecipe>natural_u_recipe</outrecipe>
				<throughput>1E299</throughput>
			</Source>
		</config>
		<name>mine</name>
	</facility>

	<facility>
		<config>
			<Enrichment>
				<feed_commod>natural_u</feed_commod>
				<feed_recipe>natural_u_recipe</feed_recipe>
				<product_commod>uox</product_commod>
				<tails_assay>0.003</tails_assay>
				<tails_commod>tailings</tails_commod>
				<swu_capacity>1E100</swu_capacity>
				<initial_feed>1E100</initial_feed>
			</Enrichment>
		</config>
		<name>enrichment_plant</name>
	</facility>

	<facility>
		<config>
			<Reactor>
				<assem_size>1000</assem_size>
				<cycle_time>18</cycle_time>
				<fuel_incommods>
					<val>uox</val>
				</fuel_incommods>
				<fuel_inrecipes>
					<val>uox_recipe</val>
				</fuel_inrecipes>
				<fuel_outcommods>
					<val>hot_spent_uox</val>
				</fuel_outcommods>
				<fuel_outrecipes>
					<val>spent_uox_recipe</val>
				</fuel_outrecipes>
				<n_assem_batch>1</n_assem_batch>
				<n_assem_core>3</n_assem_core>
				<power_cap>1000</power_cap>
				<refuel_time>1</refuel_time>
				<!--<lifetime>480</lifetime>-->
			</Reactor>
		</config>
		<name>lwr</name>
	</facility>

	<facility>
		<config>
			<Reactor>
				<assem_size>1000</assem_size>
				<cycle_time>18</cycle_time>
				<fuel_incommods>
					<val>mox</val>
				</fuel_incommods>
				<fuel_inrecipes>
					<val>mox_recipe</val>
				</fuel_inrecipes>
				<fuel_outcommods>
					<val>hot_spent_mox</val>
				</fuel_outcommods>
				<fuel_outrecipes>
					<val>spent_mox_recipe</val>
				</fuel_outrecipes>
				<n_assem_batch>1</n_assem_batch>
				<n_assem_core>3</n_assem_core>
				<power_cap>1000</power_cap>
				<refuel_time>1</refuel_time>
				<!--<lifetime>480</lifetime>-->
			</Reactor>
		</config>
		<name>sfr</name>
	</facility>

	<facility>
		<config>
			<Storage>
				<in_commods>
					<val>hot_spent_uox</val>
				</in_commods>
				<out_commods>
					<val>cool_spent_uox</val>
				</out_commods>
				<residence_time>36</residence_time>
				<throughput>1E299</throughput>
				<max_inv_size>1E299</max_inv_size>
			</Storage>
		</config>
		<name>uox_cooling_pool</name>
	</facility>

	<facility>
		<config>
			<Storage>
				<in_commods>
					<val>hot_spent_mox</val>
				</in_commods>
				<out_commods>
					<val>cool_spent_mox</val>
				</out_commods>
				<residence_time>36</residence_time>
				<throughput>1E299</throughput>
				<max_inv_size>1E299</max_inv_size>
			</Storage>
		</config>
		<name>mox_cooling_pool</name>
	</facility>

	<facility>
		<config>
			<Separations>
				<feed_commods>
					<val>cool_spent_uox</val>
				</feed_commods>
				<feed_commod_prefs>
					<val>1.0</val>
				</feed_commod_prefs>
				<feed_recipe>spent_uox_recipe</feed_recipe>
				<feedbuf_size>1E100</feedbuf_size>
				<leftover_commod>uox_reprocessing_waste</leftover_commod>
				<leftoverbuf_size>1E100</leftoverbuf_size>
				<streams>
					<item>
						<commod>uox_Pu</commod>
						<info>
							<buf_size>1E100</buf_size>
							<efficiencies>
								<item>
									<comp>Pu239</comp>
									<eff>0.998</eff>
								</item>
							</efficiencies>
						</info>
					</item>
					<item>
						<commod>uox_U</commod>
						<info>
							<buf_size>1E100</buf_size>
							<efficiencies>
								<item>
									<comp>U238</comp>
									<eff>0.998</eff>
								</item>
							</efficiencies>
						</info>
					</item>
				</streams>
			</Separations>
		</config>
		<name>uox_reprocessing_plant</name>
	</facility>

	<facility>
		<config>
			<Separations>
				<feed_commods>
					<val>cool_spent_mox</val>
				</feed_commods>
				<feed_commod_prefs>
					<val>1.0</val>
				</feed_commod_prefs>
				<feed_recipe>spent_mox_recipe</feed_recipe>
				<feedbuf_size>1E100</feedbuf_size>
				<leftover_commod>mox_reprocessing_waste</leftover_commod>
				<leftoverbuf_size>1E100</leftoverbuf_size>
				<streams>
					<item>
						<commod>mox_Pu</commod>
						<info>
							<buf_size>1E100</buf_size>
							<efficiencies>
								<item>
									<comp>Pu239</comp>
									<eff>0.998</eff>
								</item>
							</efficiencies>
						</info>
					</item>
					<item>
						<commod>mox_U</commod>
						<info>
							<buf_size>1E100</buf_size>
							<efficiencies>
								<item>
									<comp>U238</comp>
									<eff>0.998</eff>
								</item>
							</efficiencies>
						</info>
					</item>
				</streams>
			</Separations>
		</config>
		<name>mox_reprocessing_plant</name>
	</facility>	

	<facility>
		<config>
			<Mixer>
				<in_streams>
					<stream>
						<info>
							<mixing_ratio>0.129</mixing_ratio>
							<buf_size>1E100</buf_size>
						</info>
						<commodities>
							<item>
								<commodity>uox_Pu</commodity>
								<pref>1.0</pref>
							</item>
							<item>
								<commodity>mox_Pu</commodity>
								<pref>2.0</pref>
							</item>
						</commodities>
					</stream>
					<stream>
						<info>
							<mixing_ratio>0.714</mixing_ratio>
							<buf_size>1E100</buf_size>
						</info>
						<commodities>
							<item>
								<commodity>uox_U</commodity>
								<pref>1.0</pref>
							</item>
							<item>
								<commodity>mox_U</commodity>
								<pref>1.0</pref>
							</item>
						</commodities>
					</stream>
					<stream>
						<info>
							<mixing_ratio>0.157</mixing_ratio>
							<buf_size>1E100</buf_size>
						</info>
						<commodities>
							<item>
								<commodity>tailings</commodity>
								<pref>2.0</pref>
							</item>
							<item>
								<commodity>natural_u</commodity>
								<pref>1.0</pref>
							</item>
						</commodities>
					</stream>
				</in_streams>
				<out_commod>mox_fuel</out_commod>
				<out_buf_size>1E100</out_buf_size>
			</Mixer>
		</config>
		<name>mox_mixer</name>
	</facility>

	<facility>
		<config>
			<Sink>
				<in_commods>
					<val>uox_reprocessing_waste</val>
					<val>mox_reprocessing_waste</val>
					<val>tailings</val>
				</in_commods>
				<max_inv_size>1E299</max_inv_size>
			</Sink>
		</config>
		<name>dgr</name>
	</facility>

	<!-- Regions -->

	<region>
		<config>
			<NullRegion/>
		</config>
		<institution>
			<config>
				<NullInst/>
			</config>
			<initialfacilitylist>
				<entry>
					<prototype>mine</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>enrichment_plant</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>lwr</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>uox_cooling_pool</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>mox_cooling_pool</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>uox_reprocessing_plant</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>mox_reprocessing_plant</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>mox_mixer</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>dgr</prototype>
					<number>1</number>
				</entry>
			</initialfacilitylist>
			<name>test_institution</name>
		</institution>
		<institution>
			<config>
				<DemandDrivenDeploymentInst>
					<calc_method>ma</calc_method>
					<facility_commod>
						<item>
							<facility>sfr</facility>
							<commod>POWER</commod>
						</item>
					</facility_commod>
					<facility_capacity>
						<item>
							<facility>sfr</facility>
							<capacity>1000</capacity>
						</item>
					</facility_capacity>
					<demand_eq>60000</demand_eq>
				</DemandDrivenDeploymentInst>
			</config>
			<name>d3ploy_institution</name>
		</institution>
		<name>big_region</name>
	</region>

