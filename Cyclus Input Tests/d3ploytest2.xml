<simulation>
	<archetypes>
		<spec>
			<lib>agents</lib>
			<name>NullRegion</name>
		</spec>
		<spec>
			<lib>agents</lib>
			<name>NullInst</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Source</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Reactor</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Sink</name>
		</spec>
		<spec>
			<lib>d3ploy.demand_driven_deployment_inst</lib>
			<name>DemandDrivenDeploymentInst</name>
		</spec>
	</archetypes>

	<control>
		<duration>50</duration>
		<startmonth>1</startmonth>
		<startyear>2000</startyear>
	</control>

	<facility>
		<config>
			<Source>
				<outcommod>uox</outcommod>
				<outrecipe>uox_recipe</outrecipe>
				<throughput>5000</throughput>
			</Source>
		</config>
		<name>mine</name>
	</facility>

	<facility>
		<config>
			<Reactor>
				<assem_size>1000</assem_size>
				<cycle_time>5</cycle_time>
				<fuel_incommods>
					<val>uox</val>
				</fuel_incommods>
				<fuel_inrecipes>
					<val>uox_recipe</val>
				</fuel_inrecipes>
				<fuel_outcommods>
					<val>spent_uox</val>
				</fuel_outcommods>
				<fuel_outrecipes>
					<val>spent_uox_recipe</val>
				</fuel_outrecipes>
				<n_assem_batch>1</n_assem_batch>
				<n_assem_core>3</n_assem_core>
				<power_cap>1000</power_cap>
				<refuel_time>0</refuel_time>
			</Reactor>
		</config>
		<lifetime>10</lifetime>
		<name>lwr</name>
	</facility>

	<facility>
		<config>
			<Sink>
				<in_commods>
					<val>spent_uox</val>
				</in_commods>
				<max_inv_size>10000</max_inv_size>
			</Sink>
		</config>
		<name>dgr</name>
	</facility>

<region>
		<config>
			<NullRegion/>
		</config>
		<institution>
			<config>
				<NullInst/>
			</config>
			<initialfacilitylist>
				<entry>
					<prototype>mine</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>lwr</prototype>
					<number>1</number>
				</entry>
				<entry>
					<prototype>dgr</prototype>
					<number>1</number>
				</entry>
			</initialfacilitylist>
			<name>initial_institution</name>
		</institution>
		<institution>
			<config>
				<DemandDrivenDeploymentInst>
					<calc_method>ma</calc_method>
					<facility_commod>
						<item>
							<facility>mine</facility>
							<commod>uox</commod>
						</item>
						<item>
							<facility>lwr</facility>
							<commod>POWER</commod>
						</item>
						<item>
							<facility>dgr</facility>
							<commod>spent_uox</commod>
						</item>
					</facility_commod>
					<facility_capacity>
						<item>
							<facility>mine</facility>
							<capacity>5000</capacity>
						</item>
						<item>
							<facility>lwr</facility>
							<capacity>1000</capacity>
						</item>
						<item>
							<facility>dgr</facility>
							<capacity>10000</capacity>
						</item>
					</facility_capacity>
					<demand_eq>20000</demand_eq>
				</DemandDrivenDeploymentInst>
			</config>
			<name>nuclear_front_end</name>
		</institution>
		<name>small_region</name>
	</region>

